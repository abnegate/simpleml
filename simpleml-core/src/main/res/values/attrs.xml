<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CameraOverlay">
        <attr name="overlay_alpha" format="float" />
        <attr name="overlay_borderWidth" format="integer" />
    </declare-styleable>

    <declare-styleable name="Camera2View">
        <attr name="simpleml_minimumConfidence" format="float" />
        <attr name="simpleml_detectorMode" format="enum">
            <enum name="frame" value="0" />
            <enum name="frame_stream" value="1" />
        </attr>
        <attr name="simpleml_analysisLocation" format="enum">
            <enum name="device" value="0" />
            <enum name="firebase_vision" value="1" />
        </attr>
        <attr name="simpleml_detectionDispatcher" format="enum">
            <enum name="main" value="0" />
            <enum name="IO" value="1" />
            <enum name="unconfined" value="2" />
        </attr>
        <attr name="simpleml_classificationEnabled" format="boolean" />
        <attr name="simpleml_detectMultiple" format="boolean" />
    </declare-styleable>
</resources>
